<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Asian Taste
			{% endblock %}
		</title>
		<link
		rel="icon" href="{{ asset('build/images/rice.png') }}">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<header>
			{% include 'components/_navbar.html.twig' %}
		</header>
		
		{% block body %}{% endblock %}
		
		<footer>
			{% include 'components/_footer.html.twig' %}
		</footer>
	</body>

</html>
